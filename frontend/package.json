{
  "name": "react-app",
  "version": "0.0.0",
  "description": "redux learning",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.ts",
    "start": "webpack serve --config webpack.dev.config.ts",
    "tsc": "npx tsc --noEmit",
    "tsc:watch": "npx tsc --watch",
    "lint": "npx eslint src/*",
    "cy:dev:open": "cypress open --config-file ./cypress/dev/cypress.config.ts --browser firefox",
    "cy:dev:ci": "npx cypress run  --config-file ./cypress/dev/cypress.config.ts",
    "cy:predeploy:ci": "npx cypress run --config-file ./cypress/deploy/cypress.config.ts",
    "prettier": "npx prettier src --check",
    "test": "jest ./src/*",
    "test:watch": "jest --watch ./src/*",
    "test:components": "jest --watch  ./src/ui/*",
    "check:prj": "run-s tsc test lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "cd .. && husky install frontend/.husky"
  },
